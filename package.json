{
  "name": "foxman",
  "version": "0.8.0-alpha",
  "description": "Not Only Mock Server",
  "main": "app.js",
  "bin": {
    "foxman": "bin/foxman"
  },
  "directories": {
    "test": "test"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "scripts": {
    "build": "babel src -w -d dist -s -x .es6",
    "dev": "babel src -w -d src -s -x .es6",
    "test": "babel src -d src -s -x .es6 &&  mocha ./test",
    "release": "git add -A && git commit -m \"Release\" && git push && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxman-tools/foxman.git"
  },
  "keywords": [
    "nodejs",
    "mock",
    "proxy"
  ],
  "author": "junyu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxman-tools/foxman/issues"
  },
  "homepage": "https://github.com/foxman-tools/foxman#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-3": "^6.22.0"
  },
  "dependencies": {
    "anymatch": "^1.3.0",
    "babel-runtime": "^6.20.0",
    "chokidar": "^1.6.0",
    "colors": "^1.1.2",
    "globule": "^1.0.0",
    "http-proxy": "^1.15.2",
    "http2": "^3.3.6",
    "koa": "^1.2.1",
    "koa-bodyparser": "^2.3.0",
    "koa-ejs": "^3.0.0",
    "koa-serve": "^0.1.7",
    "nei": "^3.0.3",
    "path-to-regexp": "^1.5.3",
    "validate": "^3.0.1",
    "vinyl-fs": "^0.3.14",
    "vinyl-fs-diff": "^1.0.0",
    "ws": "^1.1.1",
    "yargs": "^4.8.1",
    "fast-ftl": "latest"
  }
}
