{
  "name": "foxman",
  "version": "0.4.4",
  "description": "前端集成开发工具",
  "main": "app.js",
  "bin": {
    "foxman": "bin/foxman"
  },
  "directories": {
    "test": "test"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "scripts": {
    "build": "babel src -d dist -s -x .es6",
    "dev": "babel src -w -d src -s -x .es6",
    "test": "mocha ./test/test",
    "pub": "git add -A && git commit -m \"发布版本\" && git push && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxman-tools/foxman.git"
  },
  "keywords": [
    "nodejs",
    "mock",
    "proxy"
  ],
  "author": "junyu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxman-tools/foxman/issues"
  },
  "homepage": "https://github.com/foxman-tools/foxman#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2"
  },
  "dependencies": {
    "anymatch": "^1.3.0",
    "babel-polyfill": "^6.13.0",
    "chokidar": "^1.6.0",
    "colors": "^1.1.2",
    "freemarker": "^1.1.1",
    "globule": "^1.0.0",
    "http-proxy": "^1.15.2",
    "koa": "^1.2.1",
    "koa-ejs": "^3.0.0",
    "koa-logger": "^1.3.0",
    "koa-serve": "^0.1.7",
    "nei": "^3.0.3",
    "object-assign": "^4.1.0",
    "path-to-regexp": "^1.5.3",
    "vinyl-fs": "^0.3.14",
    "ws": "^1.1.1",
    "yargs": "^4.8.1"
  }
}
