{
  "name": "foxman",
  "version": "0.8.0-beta.14",
  "description": "可扩展的 Mock Server",
  "main": "app.js",
  "bin": {
    "foxman": "./dist/CommandLine/index.js"
  },
  "directories": {
    "test": "test"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "scripts": {
    "build": "gulp && babel src -d dist -x .es6",
    "builddev": "gulp && babel src -w -d dist -x .es6",
    "dev": "gulp && babel src -w -d src -s -x .es6",
    "test": "gulp && babel src -d dist -s -x .es6 &&  mocha ./test",
    "release": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxman-tools/foxman.git"
  },
  "keywords": [
    "nodejs",
    "mock",
    "proxy"
  ],
  "author": "junyu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxman-tools/foxman/issues"
  },
  "homepage": "https://github.com/foxman-tools/foxman#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.13.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2"
  },
  "dependencies": {
    "anymatch": "^1.3.0",
    "babel-runtime": "^6.20.0",
    "chalklog": "^1.1.2",
    "chokidar": "^1.6.0",
    "co": "^4.6.0",
    "fast-ftl": "latest",
    "globule": "^1.0.0",
    "http-proxy": "^1.15.2",
    "http2": "^3.3.6",
    "koa": "^1.2.1",
    "koa-bodyparser": "^2.3.0",
    "koa-ejs": "^3.0.0",
    "koa-static-cache": "^4.0.0",
    "nei": "^3.0.3",
    "node-notifier": "^5.1.2",
    "path-to-regexp": "^1.5.3",
    "vinyl-fs": "^0.3.14",
    "ws": "^1.1.1",
    "yargs": "^4.8.1"
  }
}
