// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "files.exclude":{
        "**/._*": true,
        "node_modules/": true,
        "src/**/*.js.map": true,
        "src/**/*.js": true,
        "example/node_modules/": true,
        "**/npm-debug*": true
    },
    "javascript.validate.enable": false,
    "javascript.validate.semanticValidation": false,
    "javascript.validate.syntaxValidation": true
}